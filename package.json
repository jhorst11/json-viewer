{
    "name": "@alenaksu/json-viewer",
    "version": "0.0.6",
    "module": "build/index.js",
    "main": "build/index.js",
    "source": "src/index.js",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alenaksu/json-viewer.git"
    },
    "keywords": [
        "json",
        "viewer",
        "webcomponents"
    ],
    "author": "alenaksu",
    "bugs": {
        "url": "https://github.com/alenaksu/json-viewer/issues"
    },
    "scripts": {
        "start": "parcel ./index.html",
        "build": "rm -rf dist && rollup -c rollup.config.js",
        "demo": "rm -rf docs/ && parcel build index.html --public-url /json-viewer/ -d docs/"
    },
    "dependencies": {
        "@tiny-lit/element": "^2.0.0-alpha.1",
        "@webcomponents/custom-elements": "^1.3.1"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-modules": "^0.1.3",
        "husky": "^4.2.3",
        "parcel": "^2.0.0-alpha.3.2",
        "postcss-modules": "^1.5.0",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "rollup": "^1.32.0",
        "rollup-plugin-babel": "^4.3.3",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^5.2.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "@pika/pack": {
        "pipeline": [
            [
                "@pika/plugin-standard-pkg"
            ],
            [
                "@pika/plugin-build-web"
            ]
        ]
    },
    "browserslist": [
        "chrome > 50"
    ]
}
